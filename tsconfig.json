{
  "compileOnSave": false,
  "compilerOptions": {
    "strictPropertyInitialization": false,
    "outDir": "./dist/out-tsc",
    "esModuleInterop": true,
    "baseUrl": "src",
    "sourceMap": true,
    "strict": false,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"]
  }
}
